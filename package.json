{
  "name": "nemo-agent-toolkit",
  "version": "1.0.0",
  "description": "NeMo Agent Toolkit - SRE Dashboard with AI-powered insights",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "start": "next start --port 3000",
    "lint": "next lint",
    "backend": "cd backend && python app.py",
    "backend:dev": "cd backend && uvicorn app:app --reload --host 0.0.0.0 --port 8000",
    "setup": "python3 -m venv venv && source venv/bin/activate && pip install -r backend/requirements.txt",
    "test": "npm run lint && npm run build"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "next": "^14.0.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.3.6",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "typescript": "^5.0.0"
  }
}
